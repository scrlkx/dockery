{
    "pythonVersion": "3.13",
    "pythonPlatform": "Linux",
    "typeCheckingMode": "strict",
    "reportMissingModuleSource": false,
    "include": [
        "src"
    ],
    "exclude": [
        "**/__pycache__",
        "**/site-packages",
        "**/flatpak-build-dir",
        "**/.flatpak-builder"
    ],
    "stubPath": "typings",
    "useLibraryCodeForTypes": true
}
